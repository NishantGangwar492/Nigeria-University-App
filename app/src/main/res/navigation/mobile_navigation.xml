        <action
            android:id="@+id/action_nav_home_to_schoolAnthem"
            app:destination="@id/schoolAnthem" />
        <action
            android:id="@+id/action_nav_home_to_articals"
            app:destination="@id/articals" />
        <action
            android:id="@+id/action_nav_home_to_syllabus"
            app:destination="@id/syllabus" />
        <action
            android:id="@+id/action_nav_home_to_questionFragment"
            app:destination="@id/questionFragment" />
        <action
            android:id="@+id/action_nav_home_to_takeExamFragment"
            app:destination="@id/takeExamFragment" />
        <action
            android:id="@+id/action_nav_home_to_premiumProcessFragment"
            app:destination="@id/premiumProcessFragment" />
