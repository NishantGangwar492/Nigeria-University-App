        android:name="com.iotait.schoolapp.ui.homepage.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home">

        <action
            android:id="@+id/action_nav_home_to_unnNews"
            app:destination="@id/unnNews" />
        <action
