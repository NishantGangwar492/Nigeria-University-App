            android:id="@+id/action_navigation_to_nav_home"
            app:destination="@id/nav_home"
            app:enterAnim="@anim/zoom_in"
            app:exitAnim="@anim/slide_from_right_animation"
            app:popEnterAnim="@anim/zoom_out"
            app:popExitAnim="@anim/slide_from_left_animation"/>
        <action
            android:id="@+id/action_navigation_to_schoolAnthem"
            app:destination="@id/schoolAnthem" />
        <action
            android:id="@+id/action_navigation_to_unnNews"
            app:destination="@id/unnNews" />
        <action
            android:id="@+id/action_navigation_to_articals"
            app:destination="@id/articals" />
        <action
            android:id="@+id/action_navigation_to_syllabus"
            app:destination="@id/syllabus" />
        <action
            android:id="@+id/action_navigation_to_questionFragment"
            app:destination="@id/questionFragment" />
        <action
            android:id="@+id/action_navigation_to_takeExamFragment"
            app:destination="@id/takeExamFragment" />
        <action
            android:id="@+id/action_navigation_to_premiumProcessFragment"
            app:destination="@id/premiumProcessFragment" />
